SELECT "orders__status" "orders__status", date_trunc('day', "orders__created_at_day") "orders__created_at_day", sum("orders__count") "orders__count" 
FROM  orders__segment_rollup  AS "orders__segment_rollup"
GROUP BY 1, 2
ORDER BY  2  ASC
